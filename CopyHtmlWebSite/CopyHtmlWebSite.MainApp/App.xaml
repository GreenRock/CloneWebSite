<unity:PrismApplication x:Class="CopyHtmlWebSite.MainApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:unity="http://prismlibrary.com/"
             xmlns:valueConverter="clr-namespace:CopyHtmlWebSite.MainApp.ValueConverter">
    <Application.Resources>
       <ResourceDictionary>
            <valueConverter:InvertBooleanConverter x:Key="InvertBooleanConvert"/>
            <valueConverter:StringNotNullOrEmptyConverter x:Key="StringNotNullOrEmptyConverter"/>
            <valueConverter:StringNullOrEmptyConverter x:Key="StringNullOrEmptyConverter"/>
            <valueConverter:ObjectNotNullConverter x:Key="ObjectNotNullConverter"/>
            <valueConverter:ObjectNullConverter x:Key="ObjectNullConverter"/>
            <valueConverter:ListNotNullOrEmptyConverter x:Key="ListNotNullOrEmptyConverter"/>
            <valueConverter:ListNullOrEmptyConverter x:Key="ListNullOrEmptyConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</unity:PrismApplication>
